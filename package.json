  "main": "app/background.js",
  "scripts": {
    "dev": "nextron",
    "dev:desktop": "nextron",
    "dev:web": "cd renderer && next dev -p 3002",
    "build": "nextron build",
    "build:desktop": "nextron build",
    "build:web": "cd renderer && next build && next export",
    "start": "next start",
    "start:web": "cd renderer && next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:hybrid": "node test-hybrid.js",
    "build:all": "nextron build --all",
    "db:local:generate": "prisma generate --schema=prisma/schema.local.prisma",
    "db:local:push": "prisma db push --schema=prisma/schema.local.prisma",
    "db:local:studio": "prisma studio --schema=prisma/schema.local.prisma",
    "db:local:reset": "prisma db push --schema=prisma/schema.local.prisma --force-reset",
    "db:generate:all": "npm run db:generate && npm run db:local:generate",
    "test:coverage": "jest --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:seed": "prisma db seed",
    "prisma:studio": "prisma studio",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "deploy:vercel": "vercel --prod",
    "postinstall": "npx --yes prisma@6.8.2 generate"
  },
