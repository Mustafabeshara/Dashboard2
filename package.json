    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "baseline-browser-mapping": "^2.8.32",
    "concurrently": "^9.1.2",
    "cross-env": "^10.1.0",
    "electron": "^35.7.5",
    "electron-builder": "^25.1.8",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.5",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^15.2.11",
    "msw": "^2.12.3",
    "prettier": "^3.7.4",
    "prisma": "^6.1.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.5",
    "tsx": "^4.19.4",
    "typescript": "^5.7.2",
    "wait-on": "^8.0.1",
    "postcss": "^8.4.49",
    "autoprefixer": "^10.4.20"
  },
  "build": {
    "extends": "electron-builder.json"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  }
