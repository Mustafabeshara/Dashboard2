    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.4",
    "recharts": "^2.15.0",
    "redis": "^5.10.0",
    "sonner": "^1.7.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.30.3",
    "tailwind-merge": "^2.6.0",
    "uuid": "^11.1.0",
