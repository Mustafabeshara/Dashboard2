{
  "name": "medical-distribution-system",
  "version": "1.0.0",
  "private": true,
  "description": "Medical Distribution Dashboard - Desktop Application with AI Features",
  "author": {
    "name": "Beshara Group",
    "email": "m.beshara@besharagroup.com"
  },
  "homepage": "https://github.com/Mustafabeshara/Dashboard2#readme",
  "bugs": {
    "url": "https://github.com/Mustafabeshara/Dashboard2/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mustafabeshara/Dashboard2.git"
