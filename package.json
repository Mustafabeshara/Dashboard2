{
  "name": "medical-distribution-system",
  "version": "1.0.0",
  "private": true,
  "description": "Medical Distribution Dashboard - Desktop Application with AI Features",
  "author": {
    "name": "Beshara Group",
    "email": "m.beshara@besharagroup.com"
  },
  "homepage": "https://github.com/Mustafabeshara/Dashboard2#readme",
  "bugs": {
    "url": "https://github.com/Mustafabeshara/Dashboard2/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mustafabeshara/Dashboard2.git"
  },
  "license": "UNLICENSED",
  "engines": {
    "node": ">=20.9.0"
  },
  "main": "electron/main.js",
  "scripts": {
    "dev": "next dev",
    "dev:stable": "NODE_OPTIONS='--max-old-space-size=4096' next dev --turbo",
    "build": "prisma generate && prisma migrate deploy && next build",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "validate:env": "tsx scripts/validate-env.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "electron:builder": "electron-builder",
    "electron:builder:mac": "electron-builder --mac",
    "electron:builder:win": "electron-builder --win",
    "electron:builder:linux": "electron-builder --linux",
    "db:local:generate": "prisma generate --schema=./prisma/schema.prisma",
    "db:local:push": "prisma db push --schema=./prisma/schema.prisma",
    "db:local:studio": "prisma studio --schema=./prisma/schema.prisma",
    "db:local:reset": "prisma migrate reset --schema=./prisma/schema.prisma",
    "db:generate:all": "npm run db:generate && npm run db:local:generate",
    "test:coverage": "jest --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:seed": "prisma db seed",
    "prisma:studio": "prisma studio",

    "postinstall": "prisma generate",
    "prepare": "husky",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",

    "setup:database": "./scripts/setup-database.sh",
    "test:ai": "node scripts/test-ai-providers.js",
    "test:health": "node scripts/health-check.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.940.0",
